{
  "name": "pathfinder",
  "displayName": "pathfinder",
  "description": "PathFinder is an application that analyzes function calls within your code and generates a graph of your source code enriched with various extra details. With this extension, you can accelerate your secure code review processes and analyze your function calls more quickly and clearly.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fatihhcelik/PathFinder"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "publisher": "fatihhcelik",
  "icon": "demo/sc.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "callgraph.analyze",
        "title": "Analyze Call Graph"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
